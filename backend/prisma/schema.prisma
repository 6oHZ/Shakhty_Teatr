datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Play {
  id          Int      @id @default(autoincrement())
  title       String
  genre       String?
  director    String?
  description String?  @db.Text
  duration    String?
  ageLimit    String?
  createdAt   DateTime @default(now())
}

model Admins {
  id          Int     @id @default(autoincrement())
  login       String
  password    String
  createdAt   DateTime  @default(now())
}